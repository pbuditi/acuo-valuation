	<!-- Equity swaps may also be ATS-like which can be terminated by either party
					  after a given notice period-->
	<!--
			  A swaption is basically just a wrapper round a swap, with additional
			  definitions for the option.  Note that although it cannot be enforced
			  in the schema, the system requires that the tradeid and book elements on
			  the underlying swap be the same as on the swaption.

			  Also note that for now we only support underlying swaps where the fixed
			  and floating leg currencies are the same.
			 -->
	<xs:complexType name="swaption-info">
		<xs:complexContent>
			<xs:extension base="trade-date-info">
				<xs:sequence>
					<xs:element name="swap" type="swap-info"/>
					<xs:element name="payrec" type="payorreceive"/>
					<xs:element name="notional" type="nonzero"/>
					<xs:element name="style" type="optionstyle"/>
					<xs:element name="customerpv" type="xs:double" minOccurs="0"/>
					<xs:element name="legid" type="xs:integer" minOccurs="0"/>
					<xs:element name="fee" type="fee" minOccurs="0"/>
					<xs:element name="cashsettled" type="xs:boolean" minOccurs="0"/>
					<xs:element name="discountmethod" type="xs:string" minOccurs="0"/>
					<xs:element name="discountccy" type="xs:string" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
