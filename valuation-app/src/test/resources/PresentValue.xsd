<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<!--

		   The top level element is "present-value".  This should have child elements:
			* "timezone" - a timezone close, used when asking for today's close data
			* "valuationdate" - When to evaluate the Present Value for, and
			* "data" - Containing one or more "trade" elements
		   The valuation date is restricted to past 7 calendar days, excluding today, or
		   month end for past 3 months.

		-->
	<!--
			Versioning information. This segment can be used to determine, on an automated basis, whether a change has occurred.

			Current File Revision Number ( non-sequential ) : $Rev$
			File Last Updated at : $LastChangedDate$
		-->
	<xs:element name="presentvalue">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="timezone" type="timezone" minOccurs="0"/>
				<xs:element name="valuationdate" type="xs:date"/>
				<xs:element name="valuationcurrency" type="xs:string" minOccurs="0"/>
				<xs:element name="submittedbymarkit" type="xs:boolean" default="false" minOccurs="0" maxOccurs="1"/>
				<xs:choice maxOccurs="1">
					<xs:element name="data">
						<xs:complexType>
							<xs:sequence>
								<xs:choice minOccurs="1" maxOccurs="unbounded">
									<xs:element name="cds" type="cds-info"/>
									<xs:element name="credindex" type="credindex-info"/>
									<xs:element name="irswap" type="swap-info-with-defaults"/>
									<xs:element name="credindextranche" type="credindextranche-info"/>
									<xs:element name="crossccyswap" type="swap-info"/>
									<xs:element name="inflationswap" type="swap-info"/>
									<xs:element name="swaption" type="swaption-info"/>
									<xs:element name="callableswap" type="callableswap-info"/>
									<xs:element name="exoticswap" type="exoticswap-info"/>
									<xs:element name="flexcashflow" type="flexcashflow-info"/>
									<xs:element name="irswapcustomaccrual" type="swapcustomaccrual-info"/>
									<xs:element name="exoticcapfloor" type="exoticcapfloor-info"/>
									<xs:element name="exoticrangeaccrual" type="exoticrangeaccrual-info"/>
									<xs:element name="prdc" type="prdc-info"/>
									<xs:element name="capfloor" type="capfloor-info"/>
									<xs:element name="inflationcapfloor" type="capfloor-info"/>
									<xs:element name="fx" type="fxforward-info"/>
									<xs:element name="fxswap" type="fxswap-info"/>
									<xs:element name="fxoption" type="fxoption-info"/>
									<xs:element name="credswaption" type="credswaption-info"/>
									<xs:element name="creditswapbasket" type="creditswapbasket-info"/>
									<xs:element name="cdotranche" type="cdotranche-info"/>
									<xs:element name="cdototemtranche" type="cdototemtranche-info"/>
									<xs:element name="cdoportfolio" type="cdoportfolio-info"/>
									<xs:element name="exotic" type="exotic-info"/>
									<xs:element name="bond" type="instrument-info"/>
									<xs:element name="security" type="instrument-info"/>
									<xs:element name="pricelock" type="pricelock-info"/>
									<xs:element name="tba" type="tba-info"/>
									<xs:element name="ratelock" type="ratelock-info"/>
									<xs:element name="spreadlock" type="spreadlock-info"/>
									<xs:element name="loan" type="instrument-info"/>
									<xs:element name="listedoption" type="instrument-info"/>
									<xs:element name="future" type="instrument-info"/>
									<xs:element name="cln" type="cln-info"/>
									<xs:element name="modelbond" type="cln-info"/>
									<xs:element name="equitycfd" type="swap-info"/>
									<xs:element name="equityswap" type="swap-info"/>
									<xs:element name="basketswap" type="swap-info"/>
									<xs:element name="eqvarswap" type="varianceswap-info"/>
									<xs:element name="eqvolswap" type="varianceswap-info"/>
									<xs:element name="fxvarswap" type="varianceswap-info"/>
									<xs:element name="fxvolswap" type="varianceswap-info"/>
									<xs:element name="fxvarvolswap" type="varianceswap-info"/>
									<xs:element name="fxvarbudget" type="fxvarbudget-info"/>
									<xs:element name="fxcorrelationswap" type="fxcorrswap-info"/>
									<xs:element name="eqvaroption" type="varianceoption-info"/>
									<xs:element name="equityoption" type="equity-option-info"/>
									<xs:element name="eqcliquet" type="cliquet-info"/>
									<xs:element name="correlationswap" type="corrswap-info"/>
									<xs:element name="basketoption" type="option-info"/>
									<xs:element name="equityforward" type="equityforward-info"/>
									<xs:element name="hybridcredindextranche" type="hybridcredindextranche-info"/>
									<xs:element name="lcds" type="lcds-info"/>
									<xs:element name="structfinindex" type="structfinindex-info"/>
									<xs:element name="basket" type="basket-info"/>
									<xs:element name="loancdsindex" type="loancdsindex-info"/>
									<xs:element name="abcds" type="abcds-info"/>
									<xs:element name="cmsspreadoption" type="cmsspreadoption-info"/>
									<xs:element name="cmscapfloor" type="capfloor-info"/>
									<xs:element name="cmsswap" type="swap-info"/>
									<xs:element name="commodityoption" type="commodityoption-info"/>
									<xs:element name="commodityforward" type="commodityforward-info"/>
									<xs:element name="commodityswap" type="commodityswap-info"/>
									<xs:element name="commodityvarswap" type="varianceswap-info"/>
									<xs:element name="commodityswaption" type="commodityswaption-info"/>
									<xs:element name="structproduct" type="structproduct-info"/>
									<xs:element name="structir" type="structir-info"/>
									<xs:element name="equitycash" type="equitycash-info"/>
									<xs:element name="parentinterest" type="parent-interest-info"/>
									<xs:element name="fixedincomeatt" type="fixedIncomeAttributionInfo"/>
									<xs:element name="recoverylock" type="recoverylock-info"/>
									<xs:element name="ios" type="ios-info"/>
									<xs:element name="bondoption" type="bondoption-info"/>
									<xs:element name="trx" type="trx-info"/>
									<xs:element name="perfectassetswap" type="perfectassetswap-info"/>
									<xs:element name="bondtotalreturnswap" type="bondtotalreturnswap-info"/>
									<xs:element name="basketdefinition" type="basketdefinition-info"/>
									<xs:element name="loantrs" type="loantrs-info"/>
									<xs:element name="eqvolidxoption" type="eqvolidxoption-info"/>
									<xs:element name="iboxxtrs" type="iboxxtrs-info"/>
									<xs:element name="inflationlinkedbondassetswap" type="inflationlinkedbondassetswap-info"/>
									<xs:element name="contingentcmscapfloor" type="contingentcmscapfloor-info" />
                                    <xs:element name="ascot" type="ascot-info"/>
                                    <xs:element name="genericswap" type="generic-swap-info"/>
									<xs:element name="genericbond" type="generic-bond-info" />
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--

		   The "cds" elements must have the following sub-elements, in order:
			* "tradeid" - An arbitrary trade identifier string.
			* "book" - An arbitrary book identifier string.
			* "ticker" or "red" - The reference entity identifier.
			* "currency" - The premium payment currency.
			* "docclause" - The docclause of the CDS, one of XR, MR, MM, CR, XR14, MR14, MM14, CR14
			* "tier" - The tier of the CDS, one of SNRFOR, SECDOM, SUBLT2, JRSUBUT2, PREFT1.
			* "tradedate" - The date on which the CDS was traded, in format yyyy-mm-dd.
			* "settledate" - The date on which the trade was settled, in format yyyy-mm-dd.
												(Can be adjusted or unadjusted maturity.)
			* "maturity" - The date on which the CDS matures, in format yyyy-mm-dd.
			* "rolldate" - The roll date of the trade, in format yyyy-mm-dd.  (Optional)
			* "rollcode" - The business day convention, one of FOLLOW, MODFOLL, PREV, CREDIMM.
			* "eom" - Whether to perform calculation as end of month, must be Y or N. (Defaults to N if not set.)
			* "spread" - The spread at which the trade was executed, 0.0001 being one basis point.
									   Must be greater than 0.
			* "frequency" - The premium payment frequency, one of 3M, 6M, 1Y.
			* "daycount" - The payment daycount convention, one of A360, AA, A365, 30360, 3E360.
			* "position" - The size of the trade.

		-->
	<xs:complexType name="base-info">
		<xs:sequence>
			<xs:element name="tradeid" type="xs:string"/>
			<xs:element name="clientname" type="xs:string" minOccurs="0"/>
			<xs:element name="clientinternaltradeid" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="trade-info">
		<xs:complexContent>
			<xs:extension base="base-info">
				<xs:sequence>
					<xs:element name="book" type="nonemptystring"/>
					<xs:element name="account" type="xs:string" minOccurs="0"/>
					<xs:element name="refcode" type="xs:string" minOccurs="0"/>
					<xs:element name="fund" type="xs:string" minOccurs="0"/>
					<xs:element name="counterparty" type="xs:string" minOccurs="0"/>
					<xs:element name="tradeclassifiers" type="tradeclassifiers" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="trade-date-info">
		<xs:complexContent>
			<xs:extension base="base-info">
				<xs:sequence>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="book" type="nonemptystring"/>
					<xs:element name="account" type="xs:string" minOccurs="0"/>
					<xs:element name="refcode" type="xs:string" minOccurs="0"/>
					<xs:element name="fund" type="xs:string" minOccurs="0"/>
					<xs:element name="counterparty" type="xs:string" minOccurs="0"/>
					<xs:element name="tradeclassifiers" type="tradeclassifiers" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="tradeclassifiers">
		<xs:sequence>
			<xs:element name="tradeclassifier" type="tradeclassifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="tradeclassifier">
		<xs:sequence>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="value" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="cds-info">
		<xs:complexContent>
			<xs:extension base="trade-info">
				<xs:sequence>
					<xs:choice>
						<xs:element name="ticker" type="xs:string"/>
						<xs:element name="red" type="xs:string"/>
					</xs:choice>
					<xs:element name="bond" type="instrument" minOccurs="0"/>
					<xs:element name="currency" type="xs:string" minOccurs="0"/>
					<xs:element name="docclause" minOccurs="0" type="docclause"/>
					<xs:element name="tier" minOccurs="0" type="tier"/>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="settledate" type="xs:date" minOccurs="0"/>
					<xs:element name="maturity" type="xs:date"/>
					<xs:element name="useStandardISDAValues" type="xs:boolean" minOccurs="0"/>
					<xs:element name="rolldate" type="xs:date" minOccurs="0"/>
					<xs:element name="rollcode" type="rollcodes" minOccurs="0"/>
					<xs:element name="adjust" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustStart" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustEnd" type="xs:boolean" minOccurs="0"/>
					<xs:element name="eom" minOccurs="0" type="eom"/>
					<xs:element name="spread">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="frequency" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string"/>
						</xs:simpleType>
					</xs:element>
					<xs:element name="daycount" minOccurs="0" type="daycount"/>
					<xs:element name="position" type="nonzero"/>
					<xs:element name="fixedRecovery" type="customrecovery" minOccurs="0"/>
					<xs:element name="valuationMethod" type="valuationMethod" minOccurs="0"/>
					<xs:element name="customSpread" type="xs:double" minOccurs="0"/>
					<xs:element name="customRecovery" type="xs:double" minOccurs="0"/>
					<xs:element name="customerpv" type="xs:double" minOccurs="0"/>
					<xs:element name="fee" type="fee" minOccurs="0"/>
					<xs:element name="triggerRestructuring" type="xs:boolean" minOccurs="0"/>
					<xs:element name="restructuringBucket" type="xs:string" minOccurs="0"/>
					<xs:element name="bidAskValuation" type="xs:boolean" minOccurs="0"/>
					<xs:element name="useOISYieldCurve" type="xs:boolean" minOccurs="0"/>
					<xs:element name="clearing" type="clearing-info" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="recoverylock-info">
		<xs:complexContent>
			<xs:extension base="cds-info">

			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="lcds-info">
		<xs:complexContent>
			<xs:extension base="cds-info">
				<xs:choice minOccurs="0">
					<xs:element name="cancellable" type="xs:boolean"/>
					<xs:element name="bullet" type="xs:boolean"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="credswaption-info">
		<xs:complexContent>
			<xs:extension base="trade-info">
				<xs:sequence>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="style" type="optionstyle"/>
					<xs:choice>
						<xs:element name="cds" type="cds-info"/>
						<xs:element name="credindex" type="credindex-info-simple"/>
					</xs:choice>
					<xs:element name="payrec" type="payorreceive"/>
					<xs:element name="position" type="nonzero"/>
					<xs:element name="strikerate">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minExclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="frontendprotection" type="xs:decimal" minOccurs="0"/>
					<xs:element name="customerpv" type="xs:double" minOccurs="0"/>
					<xs:element name="fee" type="fee" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="basket-info">
		<xs:sequence>
			<xs:element name="basketid" type="xs:string"/>
			<xs:element name="currency" type="xs:string"/>
			<xs:element name="returntype">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Basket"/>
						<xs:enumeration value="Portfolio"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="weightingtype" default="NOTIONAL" maxOccurs="1" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="NOTIONAL"/>
						<xs:enumeration value="SHARES"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="constituents" minOccurs="0">
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="constituent" type="basketconstituent"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:element name="initialfixingdate" type="xs:date" minOccurs="1"/>
				<xs:element name="initialfixingfactor" type="positive"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="basketdefinition-info">
		<xs:sequence>
			<xs:element name="basketid" type="xs:string"/>
			<xs:element name="currency" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="CORRELATION_SWAP"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="constituents" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:choice minOccurs="1" maxOccurs="unbounded">
							<xs:element name="constituent-equity" type="constituent-equity-info"/>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="loantrs-info">
		<xs:complexContent>
			<xs:extension base="trade-date-info">
				<xs:sequence>
					<xs:element name="maturity" type="xs:date"/>
					<xs:element name="strikeprice" type="xs:decimal"/>
					<xs:element name="position" type="nonzero"/>
					<xs:element name="identifier" type="instrument"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="iboxxtrs-info">
		<xs:complexContent>
			<xs:extension base="trade-info">
				<xs:sequence>
					<xs:choice>
						<xs:element name="isin" type="xs:string"/>
						<xs:element name="ticker" type="xs:string"/>
					</xs:choice>
					<xs:element name="currency" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="USD"/>
								<xs:enumeration value="EUR"/>
								<xs:enumeration value="GBP"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="maturity" type="xs:date"/>
					<xs:element name="initialPeriodRate" type="xs:decimal" minOccurs="0" />
					<xs:element name="floatingRate" type="xs:decimal" minOccurs="0" />
					<xs:element name="tradedIndexLevel" type="xs:decimal"/>
					<xs:element name="position" type="nonzero"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="constituent-equity-info">
		<xs:sequence>
			<xs:element name="equityid" type="instrument"/>
			<xs:element name="initialfixing" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
					</xs:restriction>
					</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="creditswapbasket-info">
		<xs:complexContent>
			<xs:extension base="trade-info">
				<xs:sequence>
					<xs:element name="portfolioid" type="xs:string"/>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="settledate" type="xs:date"/>
					<xs:element name="maturity" type="xs:date"/>
					<xs:element name="rolldate" type="xs:date" minOccurs="0"/>
					<xs:element name="rollcode" type="rollcodes" minOccurs="0"/>
					<xs:element name="adjust" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustStart" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustEnd" type="xs:boolean" minOccurs="0"/>
					<xs:element name="eom" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Y"/>
								<xs:enumeration value="N"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="spread">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="frequency" type="frequency"/>
					<xs:element name="daycount" minOccurs="0" type="daycount"/>
					<xs:element name="recovery" type="xs:double" minOccurs="0"/>
					<xs:element name="fixedRecovery" type="customrecovery" minOccurs="0"/>
					<xs:element name="position" type="nonzero"/>
					<xs:element name="currency" type="xs:string"/>
					<xs:element name="fee" minOccurs="0" type="fee"/>
					<xs:element name="bondFormat" type="xs:boolean" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="cdotranche-info">
		<xs:complexContent>
			<xs:extension base="trade-info">
				<xs:sequence>
					<xs:choice>
						<xs:element name="portfolioid" type="xs:string"/>
						<xs:element name="portfolioredid" type="xs:string"/>
					</xs:choice>
					<xs:element name="tradedate" type="xs:date"/>
					<xs:element name="settledate" type="xs:date"/>
					<xs:element name="maturity" type="xs:date"/>
					<xs:element name="rolldate" type="xs:date" minOccurs="0"/>
					<xs:element name="rollcode" type="rollcodes" minOccurs="0"/>
					<xs:element name="adjust" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustStart" type="xs:boolean" minOccurs="0"/>
					<xs:element name="adjustEnd" type="xs:boolean" minOccurs="0"/>
					<xs:element name="eom" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Y"/>
								<xs:enumeration value="N"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="attachment" maxOccurs="1" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="detachment" maxOccurs="1" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="spread">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="frequency" type="frequency"/>
					<xs:element name="daycount" minOccurs="0" type="daycount"/>
					<xs:element name="nthtodefault" type="xs:boolean" minOccurs="0"/>
					<xs:element name="recovery" type="xs:double" minOccurs="0"/>
					<xs:element name="fixedRecovery" type="customrecovery" minOccurs="0"/>
					<xs:element name="position" type="nonzero"/>
					<xs:element name="currency" type="xs:string"/>
					<xs:choice minOccurs="0" maxOccurs="2">
						<xs:element name="upfront" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
